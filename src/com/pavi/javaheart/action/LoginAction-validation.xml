<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
    "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>

	<field name="userName">
		<field-validator type="required">
			<message>
				The Name is Required.
			</message>
		</field-validator>
	</field>
	
	<field name="password">
		<field-validator type="required">
			<message>
				You Must Enter Password.
			</message>
		</field-validator>
	</field>



</validators>
